<label>New Organization </label>
<%= form_with model: @organization, local: true do |form| %>
  <div class="form-group">
    <%= form.label :name %>
    <%= form.text_field :name, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= form.submit 'Create Organization', class: 'btn btn-primary' %>
  </div>
<% end %>